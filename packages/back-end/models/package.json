{
  "name": "models",
  "version": "0.1.0",
  "description": "Template for TypeScript project using Node.js v22",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rspack build --config rspack.config.ts --mode=production && yarn build:types ",
    "build:types": "tsc --project tsconfig.types.json",
    "clean": "rimraf dist",
    "lint": "eslint --ignore-path .gitignore '**/*.{js,cjs,mjs,ts,cts,mts}'",
    "prebuild": "npm run clean",
    "start": "tsx src/main.ts",
    "test": "jest",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate"
  },
  "engines": {
    "node": "22.16.0"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.10",
    "@eslint-recommended/eslint-config-typescript": "50.1.0",
    "@jest/globals": "^30.2.0",
    "@testcontainers/postgresql": "^11.7.1",
    "@tsconfig/node22": "22.0.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "22.15.35",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.5",
    "eslint": "8.57.1",
    "glob": "11.0.3",
    "jest": "^30.2.0",
    "rimraf": "6.0.1",
    "ts-jest": "^29.4.4",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  },
  "license": "MIT",
  "dependencies": {
    "drizzle-orm": "^0.44.6",
    "pg": "^8.16.3"
  }
